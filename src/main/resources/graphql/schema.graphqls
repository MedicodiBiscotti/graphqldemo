type User {
    id: Int!
    firstName: String!
    lastName: String!
    email: String!
}

input UserInput {
    firstName: String!
    lastName: String!
    email: String!
}

type Query {
    users: [User!]!
    user(id: Int!): User
}

type Mutation {
    createUser(user: UserInput!): User!
    updateUser(id: Int!, user: UserInput!): User!
    deleteUser(id: Int!): Boolean
}
